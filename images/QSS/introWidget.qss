QLabel{border:0px;}
QPushButton
{
	border:1px solid rgb(160,160,160);
	width:100px;
	height:28px;
	color:rgb(60,60,60);
}
QPushButton:hover
{
	border:1px solid rgb(0,204,102);
	width:100px;
	height:35px;
	color:rgb(160,160,160);
}
QPushButton:pressed
{
	border:1px solid rgb(0,160,50);
	background-color:rgb(200,230,200);
	width:100px;
	height:35px;
	color:rgb(60,60,60);
}  
QTextEdit{border:0px;}  
 QScrollBar:vertical {
     border: 2px solid grey;
     background: #32CC99;
     width: 15px;
     margin: 22px 0 22px 0;
 }
 QScrollBar::handle:vertical {
     background: white;
     min-height: 20px;
 }
 QScrollBar::add-line:vertical {
     border: 2px solid grey;
     background: #32CC99;
     height: 20px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:vertical {
     border: 2px solid grey;
     background: #32CC99;
     height: 20px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     border: 2px solid grey;
     width: 3px;
     height: 3px;
     background: white;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }                                      